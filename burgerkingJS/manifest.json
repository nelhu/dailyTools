{
  "name": "decilious hamburger",
  "manifest_version": 2,
  "version": "1.0",
  "description": "an awesome plugin",
  "permissions": [
    "tabs", "http://*/*","https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*","https://tellburgerking.com.cn/Survey.aspx?*"],
      "js": ["script.js"]
    }
  ]
}